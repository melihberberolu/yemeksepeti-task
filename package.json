  "scripts": {
    "build:raw": "SET MINIFY=0 && babel-node tools/build.js",
    "build:minify": "SET MINIFY=1 && babel-node tools/build.js",
    "build": "SET MINIFY=1 && babel-node tools/build.js",
    "build:open": "babel-node tools/build.js && npm run open:dist",
    "critical": "node tools/criticalCss",
    "clean": "rimraf ./dist && mkdir dist",
    "flow:server": "#flow server",
    "generate": "plop --plopfile generators/index.js",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "open:dist_old": "babel-node tools/distServer.js",
    "open:dist": "http-server ./dist -o -p 12345 -c 3600",
    "open:src": "babel-node tools/srcServer.js",
    "prebuild": "npm run clean",
    "preinstall": "node tools/nodeVersionCheck.js",
    "prestart": "npm-run-all --parallel start-message clean",
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start": "npm-run-all --parallel open:src",
    "start-message": "babel-node tools/startMessage.js",
    "test": "SET NODE_ENV=ava && ava \"src/**/*.test.js\" --verbose",
    "test:watch": "npm run test -- --watch"
  },
